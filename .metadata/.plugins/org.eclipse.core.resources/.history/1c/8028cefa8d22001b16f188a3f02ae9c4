class Persona():
    
    _nombre=""
    _edad=0
    _residencia=""
    
    


    def __init__(self, nombre,edad,residencia):
        
        self._nombre=nombre
        self._edad=edad
        self._residencia=residencia
        
        
        
def set_nombre(self, nombre):
    self._nombre = nombre

def get_nombre(self):
    return self._nombre